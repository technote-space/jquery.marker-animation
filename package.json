  "version": "1.3.9",
  "description": "Marker animation jQuery plugin",
  "main": "jquery.marker-animation.min.js",
  "scripts": {
    "start": "yarn build",
    "test": "yarn lint && yarn jest",
    "test:update": "yarn lint && yarn jest:update",
    "lint": "eslint src/**/**/*.js && eslint __tests__/**/**/*.js",
    "jest": "jest --coverage",
    "jest:update": "jest --coverage --updateSnapshot",
    "develop": "webpack --mode development --config webpack.config.js",
    "build": "webpack --mode production --config webpack.config.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/jquery.marker-animation.git"
  },
  "keywords": [
    "jquery-plugin",
